{
  "name": "dramabox-api-nodejs",
  "version": "1.2.0",
  "description": "Dramabox API Server - REST API untuk mengakses konten Dramabox",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build:css": "tailwindcss -i ./src/styles/input.css -o ./public/css/styles.css --minify",
    "watch:css": "tailwindcss -i ./src/styles/input.css -o ./public/css/styles.css --watch"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^4.18.3",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "node-cache": "^5.1.2",
    "proxy-agent": "^6.3.1",
    "rate-limiter-flexible": "2.4.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "tailwindcss": "^3.4.17"
  }
}
